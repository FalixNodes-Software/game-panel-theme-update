{
    "manifest_version": 3,
    "name": "FalixNodes Game Panel - Theme Update", 
    "description": "Built to match up the game panel's theme with FalixNodes client area.",
    "version": "0.1",
    "author": "Korbs Studio",
    "icons": {
        "16": "16x16.png",
        "48": "48x48.png",
        "128": "128x128.png" 
    },
    "content_scripts": [
        {
          "run_at": "document_end",
          "matches": ["https://dev-panel.falixnodes.net/*"], 
          "js": ["./theme.js"],
          "css": ["./theme.css"]
        }
    ]
}